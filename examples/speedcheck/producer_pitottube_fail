# directly go to chartserver
java -Dsim -classpath .:$PILOTS_HOME/lib/* \
    pilots.util.ColumnSelectableFileInputProducer \
    $PILOTS_HOME/data/03-Apr-2012-KALB-KFME/va_fail-aa.txt \
    va:va_fail \
    localhost:9999

